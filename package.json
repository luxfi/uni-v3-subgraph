{
  "name": "@luxfi/uniswap-v3-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/luxfi/uniswap-v3-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "build:docker": "docker build -t matchstick .",
    "buildonly": "graph build",
    "deploy:alchemy": "graph deploy --node https://subgraphs.alchemy.com/api/subgraphs/deploy --ipfs https://ipfs.satsuma.xyz",
    "codegen": "graph codegen --output-dir src/types/",
    "test": "graph test -d",
    "test:no-tty": "docker run -i --rm --mount type=bind,source=$INIT_CWD,target=/matchstick matchstick",
    "create": "graph create luxfi/uniswap-v3 --node http://192.168.1.165:8020",
    "deploy": "RUST_LOG=debug graph deploy luxfi/uniswap-v3 --ipfs http://192.168.1.165:5555 --node http://192.168.1.165:8020",
    "watch": "graph deploy luxfi/uniswap-v3 --watch --debug --node http://192.168.1.165:8020 --ipfs http://192.168.1.165:5555"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.1.0",
    "matchstick-as": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
